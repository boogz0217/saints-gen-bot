# Discord Bot Token - Get this from Discord Developer Portal
DISCORD_TOKEN=your_bot_token_here

# Admin Discord User IDs (comma-separated) - These users can generate/revoke licenses
# Find your ID: Enable Developer Mode in Discord, right-click yourself, Copy ID
ADMIN_IDS=123456789012345678,987654321098765432

# Secret key for signing license keys - USE A STRONG RANDOM STRING!
# This MUST match the key in the macro app's license_manager.py
# Generate one with: python -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=your_super_secret_key_here

# Database path (optional, defaults to licenses.db)
DATABASE_PATH=licenses.db

# Guild (Server) ID - Right-click your server name, Copy ID
GUILD_ID=1290387028185448469

# Subscriber Role ID - Right-click the role, Copy ID
# This role is given when a license is generated and removed when it expires
SUBSCRIBER_ROLE_ID=1234567890123456789

# Store URL for renewal messages
STORE_URL=https://saintservice.store/
